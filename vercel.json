{
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/tinymce/(.*)",
      "dest": "/public/tinymce/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
